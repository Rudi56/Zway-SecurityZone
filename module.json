{
   "author" : "Maroš Kollár",
   "category" : "complex_applications",
   "defaults" : {
      "delay_activate" : 0,
      "delay_alarm" : 0,
      "description" : "__description__",
      "timeout" : 0,
      "title" : "__title__"
   },
   "dependencies" : [],
   "homepage" : "http://k-1.com",
   "icon" : "icon.png",
   "maturity" : "stable",
   "moduleName" : "SecurityZone",
   "options" : {
      "fields" : {
         "delay_activate" : {
            "helper" : "__delay_activate_helper__",
            "label" : "__delay_activate_label__"
         },
         "delay_alarm" : {
            "helper" : "__delay_alarm_helper__",
            "label" : "__delay_alarm_label__"
         },
         "tests" : {
            "fields" : {
               "item" : {
                  "fields" : {
                     "testBinary" : {
                        "dependencies" : {
                           "testType" : "binary"
                        },
                        "fields" : {
                           "device" : {
                              "datasource" : "namespaces",
                              "field" : "optionLabels",
                              "label" : "",
                              "optionLabels" : "namespaces:devices_sensorBinary:deviceName,namespaces:devices_switchBinary:deviceName",
                              "type" : "select"
                           }
                        },
                        "label" : "__test_binary_label__",
                        "testValue" : {
                           "helper" : "__test_value_helper__"
                        }
                     },
                     "testMultilevel" : {
                        "dependencies" : {
                           "testType" : "multilevel"
                        },
                        "fields" : {
                           "device" : {
                              "datasource" : "namespaces",
                              "field" : "optionLabels",
                              "label" : "",
                              "optionLabels" : "namespaces:devices_sensorMultilevel:deviceName,namespaces:devices_switchMultilevel:deviceName",
                              "type" : "select"
                           },
                           "testOperator" : {
                              "optionLabels" : [
                                 "=",
                                 "≠",
                                 ">",
                                 "≥",
                                 "<",
                                 "≤"
                              ],
                              "type" : "select"
                           },
                           "testValue" : {
                              "helper" : "__test_value_helper__"
                           }
                        },
                        "label" : "__test_multi_label__"
                     },
                     "testRemote" : {
                        "dependencies" : {
                           "testType" : "remote"
                        },
                        "fields" : {
                           "device" : {
                              "datasource" : "namespaces",
                              "field" : "optionLabels",
                              "label" : "",
                              "optionLabels" : "namespaces:devices_switchControl:deviceName",
                              "type" : "select"
                           },
                           "testValue" : {
                              "helper" : "__test_value_helper__"
                           }
                        },
                        "label" : "__tests_remote_label__"
                     },
                     "testType" : {
                        "helper" : "__test_type_helper__",
                        "type" : "select"
                     }
                  }
               }
            },
            "helper" : "__tests_helper__",
            "label" : "__tests_label__"
         },
         "timeout" : {
            "helper" : "__timeout_helper__",
            "label" : "__timeout_label__"
         },
         "type" : {
            "helper" : "__type_helper__",
            "label" : "__type_label__",
            "optionLabels" : [
               "__type_intrusion__",
               "__type_flood__",
               "__type_smoke__",
               "__type_gas__",
               "__type_heat__",
               "__type_cold__",
               "__type_tamper__",
               "__type_other__"
            ]
         }
      }
   },
   "repository" : {
      "source" : "https://github.com/maros/Zway-SecurityZone",
      "type" : "git"
   },
   "schema" : {
      "properties" : {
         "delay_activate" : {
            "required" : true,
            "type" : "integer"
         },
         "delay_alarm" : {
            "required" : true,
            "type" : "integer"
         },
         "tests" : {
            "items" : {
               "properties" : {
                  "testBinary" : {
                     "dependencies" : "testType",
                     "properties" : {
                        "device" : {
                           "datasource" : "namespaces",
                           "enum" : "namespaces:devices_sensorBinary:deviceId,namespaces:devices_switchBinary:deviceId",
                           "field" : "enum",
                           "required" : true
                        },
                        "testValue" : {
                           "default" : "on",
                           "enum" : [
                              "off",
                              "on"
                           ],
                           "required" : true,
                           "type" : "string"
                        }
                     },
                     "type" : "object"
                  },
                  "testMultilevel" : {
                     "dependencies" : "testType",
                     "properties" : {
                        "device" : {
                           "datasource" : "namespaces",
                           "enum" : "namespaces:devices_sensorMultilevel:deviceId,namespaces:devices_switchMultilevel:deviceId,namespaces:devices_battery:deviceId",
                           "field" : "enum",
                           "required" : true
                        },
                        "testOperator" : {
                           "enum" : [
                              "=",
                              "!=",
                              ">",
                              ">=",
                              "<",
                              "<="
                           ],
                           "required" : true,
                           "type" : "string"
                        },
                        "testValue" : {
                           "required" : true,
                           "type" : "number"
                        }
                     },
                     "type" : "object"
                  },
                  "testRemote" : {
                     "dependencies" : "testType",
                     "properties" : {
                        "device" : {
                           "datasource" : "namespaces",
                           "enum" : "namespaces:devices_switchControl:deviceId",
                           "field" : "enum",
                           "required" : true
                        },
                        "testValue" : {
                           "default" : "on",
                           "enum" : [
                              "off",
                              "on",
                              "upstart",
                              "upstop",
                              "downstart",
                              "downstop"
                           ],
                           "required" : true,
                           "type" : "string"
                        }
                     },
                     "type" : "object"
                  },
                  "testType" : {
                     "default" : "binary",
                     "enum" : [
                        "binary",
                        "multilevel",
                        "remote"
                     ],
                     "required" : true,
                     "type" : "string"
                  }
               },
               "type" : "object"
            },
            "minItems" : 1,
            "type" : "array"
         },
         "timeout" : {
            "required" : true,
            "type" : "integer"
         },
         "type" : {
            "default" : "open",
            "enum" : [
               "intrusion",
               "flood",
               "smoke",
               "gas",
               "heat",
               "cold",
               "tamper",
               "other"
            ],
            "required" : true,
            "type" : "string"
         }
      },
      "required" : true,
      "type" : "object"
   },
   "singleton" : true,
   "version" : "1.02"
}
